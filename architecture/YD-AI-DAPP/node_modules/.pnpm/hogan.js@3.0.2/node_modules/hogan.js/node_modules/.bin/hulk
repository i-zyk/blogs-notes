#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/hogan.js@3.0.2/node_modules/hogan.js/bin/node_modules:/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/hogan.js@3.0.2/node_modules/hogan.js/node_modules:/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/hogan.js@3.0.2/node_modules:/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/hogan.js@3.0.2/node_modules/hogan.js/bin/node_modules:/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/hogan.js@3.0.2/node_modules/hogan.js/node_modules:/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/hogan.js@3.0.2/node_modules:/Users/mac/Desktop/blogs-notes/architecture/YD-AI-DAPP/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/hulk" "$@"
else
  exec node  "$basedir/../../bin/hulk" "$@"
fi
